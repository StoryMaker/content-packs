    "cards": [
        {
            "tips": [
                {
                    "text": "Ofiikee beeki, ilaalchi dogoggoraakee akka seenaakeerratti dhiibbaa hin fidne eeggadhu.", 
                    "tags": [
                        "ethics", 
                        "journalism", 
                        "during"
                    ]
                }, 
                {
                    "text": "Yeroo haala sodaachisaa keessatti waraabdutti mala ba'umsaa yeroo hunda qabaadhu.", 
                    "tags": [
                        "security", 
                        "photo", 
                        "video", 
                        "journalism", 
                        "during"
                    ]
                }, 
                {
                    "text": "Yeroo danda'ametti gaaffiif deebiin dura qorannoo tokko tokko geggeessi. Gaaffii caaluu fi baayyee barbaachisaa ta'e gaafadhu.", 
                    "tags": [
                        "journalism", 
                        "interview", 
                        "character", 
                        "video", 
                        "audio"
                    ]
                }, 
                {
                    "text": "Qaamichaaf ibsa seenaake hima tokkoon qabaadhu.", 
                    "tags": [
                        "journalism", 
                        "interview", 
                        "character", 
                        "before"
                    ]
                }, 
                {
                    "text": "Gaaffilee eeggannoo barbaadan xumuraaf kaa'adhu.", 
                    "tags": [
                        "journalism", 
                        "interview", 
                        "character", 
                        "video", 
                        "audio", 
                        "during"
                    ]
                }, 
                {
                    "text": "Gaaffiiwwankeef caalmaa kennuudhaan tokko kan odeeffannoo barbaachisaa kan ati barbaaddu adda baasi.", 
                    "tags": [
                        "journalism", 
                        "interview", 
                        "character", 
                        "video", 
                        "audio", 
                        "before"
                    ]
                }, 
                {
                    "text": "Saffisaan gara gaaffii barbaachisaa gaaffiif deebii dhaqi.", 
                    "tags": [
                        "journalism", 
                        "interview", 
                        "character", 
                        "video", 
                        "audio", 
                        "during"
                    ]
                }, 
                {
                    "text": "Gaafannookee keessatti, abshaala garuu gaarii ta'uu yaadadhu.", 
                    "tags": [
                        "journalism", 
                        "interview", 
                        "character", 
                        "video", 
                        "audio", 
                        "during"
                    ]
                }, 
                {
                    "text": "Yoo gaaffiif deebii namoota qabxii ilaalchaa garagaraa qabanii itti dabalte seenaankee baayyee gaarii ta'a.", 
                    "tags": [
                        "journalism", 
                        "interview", 
                        "character", 
                        "video", 
                        "audio", 
                        "during"
                    ]
                }, 
                {
                    "text": "Duraa duuba gaarii uumuuf waraabbii hammamtaa garagaraa qaban waraabi.", 
                    "tags": [
                        "journalism", 
                        "interview", 
                        "character", 
                        "video", 
                        "audio", 
                        "during"
                    ]
                }, 
                {
                    "text": "Gaaffii gaafachuukeen dura utuu dubbachuu hin jalqabin sekoondota yartuuf callisa waraabi.", 
                    "tags": [
                        "audio", 
                        "video", 
                        "during"
                    ]
                }, 
                {
                    "text": "Waraabuu jalqabuukeen dura dhageessistuu gurraa fayyadamuudhaan sadarkaa sagalee ilaali.", 
                    "tags": [
                        "audio", 
                        "video", 
                        "during"
                    ]
                }, 
                {
                    "text": "Hamma dandeesse sagaleekee xinneessii fayyadami, seenaakee karaa sagalee namootaa himi.", 
                    "tags": [
                        "audio", 
                        "during"
                    ]
                }, 
                {
                    "text": "Yeroo mataduree fi dhangii fayyadamtu filattutti waa'ee hordoftoota bira ga'uu barbaadduu yaadi.", 
                    "tags": [
                        "journalism", 
                        "story", 
                        "before"
                    ]
                }, 
                {
                    "text": "Yeroon daanga'aa yoo ta'eyyuu, karoora qopheeffadhu, waan argite hunda hin waraabin.", 
                    "tags": [
                        "photo", 
                        "video", 
                        "before"
                    ]
                }, 
                {
                    "text": "Yeroo gaaffiif deebii waraabdutti, yoo xiqqaate gaaffilee 15 qopheeffadhu.", 
                    "tags": [
                        "video", 
                        "audio", 
                        "journalism", 
                        "before"
                    ]
                }, 
                {
                    "text": "Seenaa gaarii fi qulqullina qabu himuuf mataduree hangafaa tokkorratti xiyyeeffadhu.", 
                    "tags": [
                        "journalism", 
                        "story", 
                        "before"
                    ]
                }, 
                {
                    "text": "Yeroo danda'ametti jalqabarratti waan argitee fi gaaffiif deebii waraabi.", 
                    "tags": [
                        "video", 
                        "audio", 
                        "journalism", 
                        "during"
                    ]
                }, 
                {
                    "text": "Yeroo gaafannoo geggeessuuf gaafattutti waa'ee matadureekee yookaan galmakee ifa godhi.", 
                    "tags": [
                        "video", 
                        "audio", 
                        "journalism", 
                        "during"
                    ]
                }, 
                {
                    "text": "Tasgabbaa'i, Tasgabbaa'uunkee akka namni wajjin gaaffiif deebii geggeessitu tasgabbaa'uuf gargaara.", 
                    "tags": [
                        "video", 
                        "audio", 
                        "journalism", 
                        "during"
                    ]
                }, 
                {
                    "text": "Yeroo hunda gaaffif deebiirraa waan barbaaddu beeki.", 
                    "tags": [
                        "video", 
                        "audio", 
                        "journalism", 
                        "during"
                    ]
                }, 
                {
                    "text": "Si'a tokkotti gaaffii tokko qofa gaafadhu.", 
                    "tags": [
                        "video", 
                        "audio", 
                        "journalism", 
                        "during"
                    ]
                }, 
                {
                    "text": "Gaagffii cimaa gaafadhu, garuu kabajaan ta'uu akka qabu yaadadhu. Gaaffiin kabajaan gaafatamu bakka fagoo ga'uu danda'a.", 
                    "tags": [
                        "video", 
                        "audio", 
                        "journalism", 
                        "during"
                    ]
                }, 
                {
                    "text": "Kabaja kennuun  amantaa uumuu fi gaafannoo fooyyessuuf si gargaaruu danda'a.", 
                    "tags": [
                        "video", 
                        "audio", 
                        "journalism", 
                        "before"
                    ]
                }, 
                {
                    "text": "Waraabduurratti cuqaasuukeen dura sagalee duubaa jeequudhaan qulqullina seenaakee miidhuu danda'u dhaggeeffadhu.", 
                    "tags": [
                        "video", 
                        "audio", 
                        "during"
                    ]
                }, 
                {
                    "text": "Gaaffilee 'cufaa' ta'an kan deebii tokko qofa qaban kan akka 'eeyyee' yookaan 'lakki' jedhu hin gaafatin.", 
                    "tags": [
                        "video", 
                        "audio", 
                        "journalism", 
                        "during"
                    ]
                }, 
                {
                    "text": "Itti fufiinsa haasichaa eegi. Gaafadhu: Maaliif? Itti aanee maaltu ta'e? Maal jechuu keeti?", 
                    "tags": [
                        "video", 
                        "audio", 
                        "journalism", 
                        "during"
                    ]
                }, 
                {
                    "text": "Gaaffiinkee deebii barbaachisaa argamsiisuu yoo dadhabe, karaa gaaffiikee jijjiiri.", 
                    "tags": [
                        "video", 
                        "audio", 
                        "journalism", 
                        "during"
                    ]
                }, 
                {
                    "text": "Odeeffannoo baayyee yoo barbaadde gaaffii hordoffii qabu fayyadami.", 
                    "tags": [
                        "video", 
                        "audio", 
                        "journalism", 
                        "during"
                    ]
                }, 
                {
                    "text": "Yaadadhu, sagalee malee fakkiinkee walakkaa seenichaa qofa hima.", 
                    "tags": [
                        "video", 
                        "photo essay", 
                        "during"
                    ]
                }, 
                {
                    "text": "Yoo qilleensa'aa jiraate, kaameeraakee qilleensarraa achi qabi, yookaan maayikiikee eegi.", 
                    "tags": [
                        "video", 
                        "audio", 
                        "during"
                    ]
                }, 
                {
                    "text": "Seenaan hundi gaaffii ijoo qaba. Xumurasaa irratti gaaffiin ati deebiste maali?", 
                    "tags": [
                        "video", 
                        "audio", 
                        "during"
                    ]
                }, 
                {
                    "text": "Dubbiin ijoo deebii gaaffii kan nama dhaggeeffatu akka dhaggeeffatu godhuudha. Daftee deebistee hin xumurin.", 
                    "tags": [
                        "journalism", 
                        "general", 
                        "audio", 
                        "during"
                    ]
                }, 
                {
                    "text": "Muraa hundaaf of gaafadhu: waan haaraa natti himu qabaa?", 
                    "tags": [
                        "journalism", 
                        "general", 
                        "during"
                    ]
                }, 
                {
                    "text": "Walitti fufiinsi gaariin suuraalee gaggaarii shan irraa qophaa'uu qaba. Fakkii gaggaarii baayyee yoo qabaatte, itti dabali.", 
                    "tags": [
                        "journalism", 
                        "general", 
                        "during"
                    ]
                }
            ], 
