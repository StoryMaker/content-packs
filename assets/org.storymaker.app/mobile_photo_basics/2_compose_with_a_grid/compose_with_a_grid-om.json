            "media": [
                {
                    "media": "3.jpg", 
                    "media_caption": "\u00a9"
                }
            ], 
            "header": "Qaama kaasuu barbaadde gidduu kaa'uudhaan bakka haferraa dhimma dhabuu hin qabdu.", 
            "references": [
                "compose_with_a_grid::milestone_start::value::true"
            ], 
            "exampleMediaPath": "3.jpg", 
            "clipType": "character", 
            "type": "ExampleCard", 
            "id": "preview_card_1"
        }, 
        {
            "text": "Tarsaan suuraakee gidduu kaa'uurra darbee akka fayyaduufi. Mee akka itti  sarara tarsaatti fayyadamuun suuraa bareedaa kaasuu akka dandeenyu haa ilaallu.", 
            "type": "BasicTextCard", 
            "id": "text_card_3", 
            "references": [
                "compose_with_a_grid::milestone_start::value::true"
            ]
        }, 
        {
