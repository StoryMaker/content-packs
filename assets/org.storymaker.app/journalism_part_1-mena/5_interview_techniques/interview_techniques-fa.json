            "header": "\u062a\u06a9\u0646\u06cc\u06a9\u200c\u0647\u0627\u06cc \u0627\u0635\u0644\u06cc \u0628\u0631\u0627\u06cc \u0628\u0647\u0631\u0647\u200c\u0645\u0646\u062f\u06cc \u0628\u06cc\u0634\u200c\u062a\u0631 \u0627\u0632 \u06cc\u06a9 \u0645\u0635\u0627\u062d\u0628\u0647 \u06a9\u062f\u0627\u0645 \u0647\u0633\u062a\u0646\u062f\u061f  ", 
            "exampleMediaPath": "1.png", 
