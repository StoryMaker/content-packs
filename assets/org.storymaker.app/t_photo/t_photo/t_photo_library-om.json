    "cards": [
        {
            "tips": [
                {
                    "text": "Gulaala xumuraatiif suuraalee 8 hamma 20tti xiyyeeffadhu.Suuraa fayyadamuuf karoorfatte baayyinaan kan caalu kaasi.", 
                    "tags": [
                        "photo essay", 
                        "after"
                    ]
                }, 
                {
                    "text": "Fakkiin haaraan hundi odeeffannoo haaraa kan seenaakee cimsu qabaachuu qabu.", 
                    "tags": [
                        "photo", 
                        "photo essay", 
                        "during"
                    ]
                }, 
                {
                    "text": "Yoo suuraa tokko qofa seenaakee keessatti fayyadamta ta'eyyuu, suuraalee garagaraa kan keessaa filachuu dandeessu kaasi.", 
                    "tags": [
                        "photo", 
                        "photo essay", 
                        "during"
                    ]
                }, 
                {
                    "text": "Ofiikee beeki, ilaalchi dogoggoraakee akka seenaakeerratti dhiibbaa hin fidne eeggadhu.", 
                    "tags": [
                        "ethics", 
                        "journalism", 
                        "during"
                    ]
                }, 
                {
                    "text": "Yeroo haala sodaachisaa keessatti waraabdutti mala ba'umsaa yeroo hunda qabaadhu.", 
                    "tags": [
                        "security", 
                        "photo", 
                        "video", 
                        "journalism", 
                        "during"
                    ]
                }, 
                {
                    "text": "Yeroon daanga'aa yoo ta'eyyuu, karoora qopheeffadhu, waan argite hunda hin waraabin.", 
                    "tags": [
                        "photo", 
                        "video", 
                        "before"
                    ]
                }, 
                {
                    "text": "Seenaa gaarii fi qulqullina qabu himuuf mataduree hangafaa tokkorratti xiyyeeffadhu.", 
                    "tags": [
                        "journalism", 
                        "story", 
                        "before"
                    ]
                }, 
                {
                    "text": "Muraa hundaaf of gaafadhu: waan haaraa natti himu qabaa?", 
                    "tags": [
                        "video", 
                        "photo", 
                        "during"
                    ]
                }, 
                {
                    "text": "Muraalee hundaaf of gaafadhu: Waa'ee gaaffii ijookoo waan natti himu qabaa?", 
                    "tags": [
                        "journalism", 
                        "general", 
                        "during"
                    ]
                }, 
                {
                    "text": "Hamma danda'ametti qaamichatti dhiyaadhu.", 
                    "tags": [
                        "photo", 
                        "during"
                    ]
                }, 
                {
                    "text": "Yaadadhu: namooti ilaalan waa'ee seenaakee waan ati suuricharratti itti agarsiisterraa kan hafe waan beekan tokkollee hin qabaani.", 
                    "tags": [
                        "photo", 
                        "during"
                    ]
                }, 
                {
                    "text": "Fakkiinkee hiikasaa ibsuu danda'aa?", 
                    "tags": [
                        "photo", 
                        "after"
                    ]
                }, 
                {
                    "text": "Seera Sadaffaan Yaadi.", 
                    "tags": [
                        "photo", 
                        "during"
                    ]
                }, 
                {
                    "text": "Gidduu goodaayyaa qofa utuu hin taane naannoo rogasaarrattis xiyyeeffadhu.", 
                    "tags": [
                        "photo", 
                        "during"
                    ]
                }, 
                {
                    "text": "Suuraa gaarii kaasuun guutummaatti waa'ee ifaati. Ifa jiru hamma siif danda'ametti akka gaariitti fayyadamaa jirtaa?", 
                    "tags": [
                        "photo", 
                        "during"
                    ]
                }, 
                {
                    "text": "Akka salphaatti kofa ittiin ilaaltu jijjiruun suuraa caalmaatti gaarii ta'e uuma.", 
                    "tags": [
                        "photo", 
                        "during"
                    ]
                }, 
                {
                    "text": "Suura tokko qofa hin kaasin. Baayyee kaasi.", 
                    "tags": [
                        "photo", 
                        "during"
                    ]
                }, 
                {
                    "text": "Yeroo danda'ametti suura mallattoo kaasuu hin irraanfatin.", 
                    "tags": [
                        "photo", 
                        "during"
                    ]
                }, 
                {
                    "text": "Suuraalee addaddaa kaafadhu -- dhiyeenyaan, gidduu fi bal'inaan kaasi.", 
                    "tags": [
                        "photo", 
                        "during"
                    ]
                }, 
                {
                    "text": "Suuraa gaariin waa'ee fiixee hojii fi agarsiisaati. Kana sammuutti qabachuudhaan yeroo filadhu.", 
                    "tags": [
                        "photo", 
                        "during"
                    ]
                }, 
                {
                    "text": "Walitti fufiinsaaf fakkiiwwankee hamma danda'ametti bifa wal fakkaatuun qophaa'uu qabu.", 
                    "tags": [
                        "series", 
                        "before"
                    ]
                }, 
                {
                    "text": "Suuraa kofa hawwataarraan kaasuun fakkicha gaarii gochuu danda'a.", 
                    "tags": [
                        "series", 
                        "during"
                    ]
                }, 
                {
                    "text": "Walitti fufiinsi gaariin suuraalee gaggaarii shan irraa qophaa'uu qaba. Fakkii gaggaarii baayyee yoo qabaatte, itti dabali.", 
                    "tags": [
                        "series", 
                        "after"
                    ]
                }
            ], 
